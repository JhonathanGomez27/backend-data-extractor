# Application
NODE_ENV=development
APP_PORT=3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=data_extractor_db

# JWT Configuration
# Access Token (short-lived)
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production
